{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongo1"
  },
  "AccessToken": {
    "dataSource": "mongo1",
    "public": false,
    "relations": {
      "client": {
        "type": "belongsTo",
        "model": "client",
        "foreignKey": "clientId"
      }
    }
  },
  "ACL": {
    "dataSource": "mongo1",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo1",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo1",
    "public": false
  },
  "Client": {
    "dataSource": "mongo1",
    "public": true
  }
}
